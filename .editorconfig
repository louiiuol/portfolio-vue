# EditorConfig is awesome (home): https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space


[*.{vue,ts}]
indent_size = 2

[*.{scss, js}]
indent_size = 2


# package.json or .travis.yml rules
[{package.json,.travis.yml}]
indent_style = space
indent_size = 2
